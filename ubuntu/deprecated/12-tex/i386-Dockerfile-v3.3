FROM ilegeul/ubuntu:i386-12.04-develop-v3.3

LABEL description="Ubuntu 12.04 i386 image, with TeX tools."
LABEL maintainer="Liviu Ionescu <ilg@livius.net>"
LABEL repository="https://github.com/xpack/xpack-build-box.git"
LABEL docs="https://github.com/xpack/xpack-build-box/tree/master/ubuntu/12-tex/README.md"

ENTRYPOINT ["linux32"]

ARG DEBUG
ARG JOBS

ARG XBB_VERSION
ARG XBB_LAYER

ADD input-ubuntu-12.04-i386 /input
RUN linux32 bash /input/build-v3.sh 2018

CMD ["/bin/bash"]
